//linear search
#include <stdio.h>

int main() {
int i,n,x,a[90];
printf("enter the number of elements:\n");
scanf("%d",&n);
printf("enter the elements:\n");
for(i=0;i<n;i++)
{
scanf("%d",&a[i]);
}
printf("enter the element to search:\n");
scanf("%d",&x);
for(i=0;i<n;i++){
if(x==a[i])
{
printf("%d is found at %d position\n",x,i+1);

break;
}
}
if(i==n)
{
printf("%d is not found",x);
}

return 0;
}
